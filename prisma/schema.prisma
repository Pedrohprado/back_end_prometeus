generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

//i need add what is prometeus

model Prometeus {
    id              String    @id @default(uuid())
    prometeusCode   String
    soldador        String
    cartao          Int
    setor           Int
    gas             String
    vazaoDoGas      String
    diametroArame   String
    velocidadeArame String
    Welding         Welding[]
    createdAt       DateTime  @default(now())
}

model Welding {
    id        String    @id @default(uuid())
    capture   String
    amperagem Int
    createdAt DateTime  @default(now())
    weldingId String
    prometeus Prometeus @relation(fields: [weldingId], references: [id])
}
